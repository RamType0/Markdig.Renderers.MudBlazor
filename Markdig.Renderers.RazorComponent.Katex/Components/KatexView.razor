@if (this.RendererInfo.IsInteractive)
{
    <InteractiveKatexView TexExpression="@TexExpression" Options="@Options"/>
}
else
{
    <StaticKatexView TexExpression="@TexExpression" Options="@Options" />
}

@code {
    [Parameter, EditorRequired]
    public required string TexExpression { get; set; }
    [Parameter]
    public KatexOptions? Options { get; set; }
}
